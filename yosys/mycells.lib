library(demo) {
  cell(buffer1) {
    area: 6;
    pin(x) { direction: input; }
    pin(z) { direction: output;
              function: "x"; }
  }
  cell(std_inv) {
    area: 3;
    pin(a) { direction: input; }
    pin(y) { direction: output;
              function: "a'"; }
  }
  cell(std_nand2) {
    area: 4;
    pin(a) { direction: input; }
    pin(b) { direction: input; }
    pin(y) { direction: output;
             function: "(a*b)'"; }
  }
  cell(std_nor2) {
    area: 4;
    pin(a) { direction: input; }
    pin(b) { direction: input; }
    pin(y) { direction: output;
             function: "(a+b)'"; }
  }
  cell(d_ff) {
    area: 18;
    ff(iq, iqn) { clocked_on: clk;
                  next_state: d; }
    pin(clk) { direction: input;
                 clock: true; }
    pin(d) { direction: input; }
    pin(q) { direction: output;
              function: "iq"; }
  }
}
